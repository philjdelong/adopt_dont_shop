<%= link_to 'Back', "/shelters" %>
<h1>Edit Shelter</h1>

<%= form_tag "/shelters/#{@shelter.id}", method: :patch do %>
  <%= label_tag :name %>
  <br>
  <%= text_field_tag :name %>
  <br><br>
  <%= label_tag :address %>
  <br>
  <%= text_field_tag :address %>
  <br><br>
  <%= label_tag :city %>
  <br>
  <%= text_field_tag :city %>
  <br><br>
  <%= label_tag :state %>
  <br>
  <%= text_field_tag :state %>
  <br><br>
  <%= label_tag :zip %>
  <br>
  <%= text_field_tag :zip %>
  <br><br>
  <%= submit_tag 'Update' %>
<% end %>
